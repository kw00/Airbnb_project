extends ../layout

block content
  .container
    h1.page-header 메모장

    table.table.table-bordered
      thead
        tr
          th 메모 목록
       tbody
        each memo in memos
          tr
            td 
              a(href='/memo/#{memo._id}') #{memo.name} 

        
    a.btn.btn-danger(href='/memo/new') 메모 작성
